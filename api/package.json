{
	"name": "seri-api",
	"version": "1.0.0",
	"scripts": {
		"build": "run-s build:clean build:code",
		"build:clean": "rimraf dist",
		"build:code": "tsc",
		"start": "node dist/entry.js",
		"dev": "nodemon src/entry.ts",
		"dev:debug": "nodemon --inspect src/entry.ts",
		"prod": "run-s build && node dist/entry.js",
		"test": "jest --no-cache"
	},
	"jest": {
		"transform": {
			"^.+\\.ts$": "ts-jest"
		},
		"testRegex": "\\.test\\.ts",
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js",
			"jsx",
			"json",
			"node"
		]
	},
	"dependencies": {
		"@typegoose/typegoose": "^7.4.8",
		"cors": "^2.8.5",
		"dotenv": "^8.2.0",
		"express": "4.17.1",
		"lodash": "^4.17.20",
		"mongoose": "^5.10.18",
		"ts-node": "9.0.0",
		"winston": "3.3.3"
	},
	"devDependencies": {
		"@types/cors": "^2.8.8",
		"@types/express": "4.17.8",
		"@types/jest": "26.0.14",
		"@types/lodash": "^4.14.161",
		"@types/mongoose": "^5.10.3",
		"@types/node": "^14.11.8",
		"@types/supertest": "2.0.10",
		"babel": "^6.23.0",
		"jest": "26.5.2",
		"nodemon": "2.0.4",
		"npm-run-all": "4.1.5",
		"prettier": "2.1.2",
		"prettier-eslint": "^11.0.0",
		"rimraf": "3.0.2",
		"supertest": "5.0.0",
		"ts-jest": "26.4.1",
		"typescript": "4.0.3"
	}
}
